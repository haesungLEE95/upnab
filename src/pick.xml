<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pickns">
	<resultMap type="pick" 		id="pickResult">
		<result property="board_num" column="board_num" />
		<result property="member_id" 		column="member_id" />
	</resultMap>
	<select id="select" parameterType="hashMap" resultType="pick">
		select * from upnab_pick where board_num=#{board_num} and member_id=#{member_id}
	</select>
	
	<insert id="insert" parameterType="hashMap">
		insert into upnab_pick values(#{board_num}, #{member_id})
	</insert>
	<delete id="delete" parameterType="hashMap">
		delete upnab_pick where board_num=#{board_num} and member_id=#{member_id}
	</delete>
	<delete id="deleteAll" parameterType="Integer">
		delete upnab_pick where board_num=#{board_num}
	</delete>	
</mapper>